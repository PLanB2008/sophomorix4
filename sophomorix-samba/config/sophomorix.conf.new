# Name of the school
SCHOOL_LONGNAME= Linus Torvalds Schule
# remove the OU from sophomorix-schools.conf:
SCHOOL_OU=LTS
# Language
LANG=DE
# Harddisk-Quota
# Using quota, should be global.sophomorix.conf
QUOTA_USE=Yes
QUOTA_DEFAULT_TEACHER=10000
QUOTA_DEFAULT_STUDENT=5000
QUOTA_DEFAULT_EXAMACCOUNT=8000
QUOTA_FILESYSTEM_ORDER=/dev/sda1,/dev/sdb2
# E-Mail-Quota
MAILQUOTA_USE=Yes
MAILQUOTA_DEFAULT_TEACHER=10000
MAILQUOTA_DEFAULT_STUDENT=5000
MAILQUOTA_DEFAULT_EXAMACCOUNT=8000
