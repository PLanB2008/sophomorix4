my $add_check=0;
my $modify_1_check=0;
my $modify_2_check=0;
my $modify_3_check=0;
my $modify_4_check=0;
my $modify_5_check=0;
my $modify_10_check=0;
my $kill_user_check=0;
my $kill_adminclasses_check=0;
my $kill_projects_check=0;

my $add_do=0;
my $modify_1_do=0;
